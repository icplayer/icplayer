<?xml version='1.0' encoding='UTF-8' ?>

<addon id="Zoom" xmlns="http://icplayer.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://icplayer.com ../../tools/addon.xsd">
    <model>
        <property name="mode" nameLabel="Zoom_property_mode" type="{Targeted area, Area around a clicked point}" />
        <property name="speechTexts" displayName="Speech texts" nameLabel="Zoom_property_speech_texts" type="staticlist">
            <property name="zoomIn" nameLabel="Zoom_property_speech_text_zoom_in" type="staticrow">
                <property name="zoomIn" nameLabel="Zoom_property_speech_text_zoom_in" type="string"/>
            </property>
            <property name="zoomOut" nameLabel="Zoom_property_speech_text_zoom_out" type="staticrow">
                <property name="zoomOut" nameLabel="Zoom_property_speech_text_zoom_out" type="string"/>
            </property>
        </property>
    </model>
</addon>
