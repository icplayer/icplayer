<?xml version='1.0' encoding='utf-8'?>
<module rename-to="icplayer">
    <inherits name="com.google.gwt.user.User"/>

    <inherits name="com.google.gwt.http.HTTP"/>
    <inherits name="com.google.gwt.xml.XML"/>
    <inherits name="com.google.gwt.json.JSON"/>
    <inherits name="com.lorepo.icf.Properties"/>
    <inherits name="com.lorepo.icf.Utils"/>
    <inherits name="com.lorepo.icf.Scripting"/>
    <inherits name="com.lorepo.icf.Screen"/>

    <stylesheet src="theme/player.css"/>
    <stylesheet src="libs/jquery-ui.css"/>
    <stylesheet src="libs/TreeGrid/css/jquery.treegrid.css"/>
    <script src="build/dist/libs.min.js"/>
    <!--Tiny mce have to be loaded separatly, due to URL finding of themes, if we change the location it will wrongly download themes-->
    <script src="libs/tinymce.min.js"/>
    <!--<script src="http://code.responsivevoice.org/responsivevoice.js"></script>-->

    <entry-point class="com.lorepo.icplayer.client.PlayerEntryPoint"/>

    <source path="client"/>

    <set-property name="user.agent" value="safari,ie9,gecko1_8, opera"/>
</module>
